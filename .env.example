# Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# Version 1.1.0 - Database Integration Support

# Disable Next.js development indicators
DISABLE_BUILD_INDICATOR=true
__NEXT_DISABLE_BUILD_INDICATOR=true

# Supabase Configuration (Required for database features)
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME="Your Site Name"

# Contact Information
NEXT_PUBLIC_CONTACT_EMAIL=contact@yoursite.com
NEXT_PUBLIC_CONTACT_PHONE=+1234567890
NEXT_PUBLIC_WHATSAPP_NUMBER=1234567890

# Social Media Links
NEXT_PUBLIC_INSTAGRAM_URL=https://www.instagram.com/yourhandle
NEXT_PUBLIC_TWITTER_URL=https://x.com/yourhandle
NEXT_PUBLIC_YOUTUBE_URL=https://www.youtube.com/@yourhandle
NEXT_PUBLIC_TIKTOK_URL=https://www.tiktok.com/@yourhandle

# Admin Configuration - CHANGE THESE VALUES!
ADMIN_EMAIL=your-admin-email@example.com
ADMIN_PASSWORD=your-secure-password-here
NEXTAUTH_SECRET=your-super-secret-key-minimum-32-characters-long

# Email Configuration for notifications
EMAIL_USER=your-email@gmail.com
EMAIL_PASS=your-app-password-here